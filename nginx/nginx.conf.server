events {}

http {
    include mime.types;

    server {
        listen 80;
        listen 443;
        server_name blogs.andrewkozinsky.ru www.blogs.andrewkozinsky.ru;

        location / {
            proxy_pass http://api:3000;
        }

        location /nginx-test {
            return 200 'Hello from Test';
        }
    }
}